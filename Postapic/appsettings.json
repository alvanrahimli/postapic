{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data source=postapic.db"
  },
  "UploadNet": {
    "UrlFormat": "https://wc.rahim.li/media/{key}",
    "Directory": "/media"
  },
  "AppConfig": {
    "BaseUrl": "https://wc.rahim.li/",
    "AuthenticateIndex": true,
    "PageSize": 10,
    "MediaConfig": {
      "MaxWidth": 1920,
      "MaxHeight": 1080
    },
    "AuthenticateWith": "github.com/themisir/identity",
    "AuthenticateWith.Alt": "cookie",
    "IdClaimName.": "postapic:id",
    "IdClaimName.Alt": "{ClaimTypes.NameIdentifier}"
  },
  "github.com/themisir/identity": {
    "Jwt": {
      "Issuer": "identity-server-url",
      "Audience": "audience"
    }
  }
}
